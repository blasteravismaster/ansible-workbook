# Execute statistics auditor every configured time  
# CRON expression (http://en.wikipedia.org/wiki/CRON_expression)
# every 60 seconds 
ch.ps.psDevice.statisticsauditor.jobSchedule=0 0 0/1 * * ?

################# PS monitoring ####################
ch.ps.psDevice.monitoring=true
service.mxagentRegistry.innerPort=4206
ch.ps.psDevice.monitoring.instance = 1

# When psDevice receives an HTTP GET request from CPE,
# psDevice sends an HTTP PUT request to the Vitria component to refresh the specific CPE data.
service.vitria.1.host=10.201.96.30
service.vitria.1.port=1800
service.vitria.userName=User
service.vitria.password=Pass

ch.ps.psDevice.visualElementHostPort=http://10.201.96.30:1796
ch.ps.psDevice.widgetLibraryHostPort=http://10.201.96.30:1796
ch.ps.psDevice.getDeviceHostPort=http://10.201.96.30:1796

# For pairing of guest 2nd screen device, define default household parameters
ch.ps.psDevice.default.household.locale.countryId=16
ch.ps.psDevice.default.household.locale.region=1
ch.ps.psDevice.default.household.locale.city=10

# The max number of IPCs allowed for one household. Default = 256
#ch.ps.psDevice.maxIpcForHousehold=<decimal value>


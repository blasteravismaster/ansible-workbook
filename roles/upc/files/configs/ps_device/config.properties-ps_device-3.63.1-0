service.psDevice.http.host=0.0.0.0
# http server port. Default value is: 8080.
service.psDevice.http.port=1796
# minimal number of http server worker threads. Default value is: 100.
service.psDevice.http.minThreads=10
#maximum number of http server worker threads. Default value is: 1000.
service.psDevice.http.maxThreads=1000
# idle time after which the connection will be closed. Default value is: 180000.
service.psDevice.http.connectionIdleTime=5000

#max number of connections to tms
service.tms.http.maxConnectionsPerAddress=500

#DB connection timeout increased from 0 to 30000
infra.connectionPool.checkoutTimeout=30000
#max number of connections to DB
infra.connectionPool.maxLimit=100

#service.psDevice.http.availabilityFilter.isEnabled
#service.psDevice.http.flowContextFilter.isEnabled
#service.psDevice.http.pingFilter.isEnabled
service.psDevice.http.monitoringFilter.isEnabled = false
#service.psDevice.http.requestValidityFilter.isEnabled

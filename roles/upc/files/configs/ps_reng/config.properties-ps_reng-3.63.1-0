service.psReng.http.host=0.0.0.0
# http server port. Default value is: 8080.
service.psReng.http.port=1792
# minimal number of http server worker threads. Default value is: 100.
service.psReng.http.minThreads=10
#maximum number of http server worker threads. Default value is: 1000.
service.psReng.http.maxThreads=500
# idle time after which the connection will be closed. Default value is: 180000.
service.psReng.http.connectionIdleTime=5000

#service.psReng.http.availabilityFilter.isEnabled
#service.psReng.http.flowContextFilter.isEnabled
#service.psReng.http.pingFilter.isEnabled
service.psReng.http.monitoringFilter.isEnabled = false
